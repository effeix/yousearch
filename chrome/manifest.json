{
  "manifest_version": 2,
  "name": "YouSearch",
  "version": "1.0.0",
  "description": "Shortcut for the YouTube search bar",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "commands": {
    "gotosearch": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Focus user cursor on search bar"
    }
  }
}
